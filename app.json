{
  "expo": {
    "name": "Hospyta",
    "slug": "Hospytal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hospyta-lg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/hospyta-lg.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Axiforma-Black.ttf",
            "./src/assets/fonts/Axiforma-Bold.ttf",
            "./src/assets/fonts/Axiforma-Book.ttf",
            "./src/assets/fonts/Axiforma-Book.ttf",
            "./src/assets/fonts/Axiforma-Heavy.ttf",
            "./src/assets/fonts/Axiforma-Light.ttf",
            "./src/assets/fonts/Axiforma-Medium.ttf",
            "./src/assets/fonts/Axiforma-Regular.ttf",
            "./src/assets/fonts/GothamPro-Bold.ttf",
            "./src/assets/fonts/GothamPro-Black.ttf",
            "./src/assets/fonts/GothamPro-Light.ttf",
            "./src/assets/fonts/GothamPro-Medium.ttf",
            "./src/assets/fonts/GothamPro.ttf",
            "./src/assets/fonts/GothamProNarrow-Bold.ttf",
            "./src/assets/fonts/GothamProNarrow-Medium.ttf",
            "./src/assets/fonts/Poppins-Bold.ttf",
            "./src/assets/fonts/Poppins-Light.ttf",
            "./src/assets/fonts/Poppins-Medium.ttf"
          ]
        }
      ]
    ]
  }
}
